        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>mousecheck = (keyboard_check_direct(mb_left))
if (272&lt;mouse_x&amp;&amp;mouse_x&lt;784) &amp;&amp; (664&lt;mouse_y&amp;&amp;mouse_y&lt;920) &amp;&amp; (mousecheck) &amp;&amp; (room == Room_Menu)
{room_goto_next();};
if menushown = 2 &amp;&amp; global.TotalShots&gt;0
{hitpercentage = round((global.TotalHits/global.TotalShots)*100)}
else 
if menushown = 2 &amp;&amp; global.TotalShots = 0
{hitpercentage = 0;}
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="8" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Main Menu     Menu 0
if menushown=0
{
    //Drawing Background Black
    draw_set_colour(c_black);
    draw_rectangle(0,0,room_width,room_height,false);

    if (272&lt;mouse_x&amp;&amp;mouse_x&lt;784) &amp;&amp; (664&lt;mouse_y&amp;&amp;mouse_y&lt;920)
        {
            draw_sprite(Spr_PlayGameButton,1,272,664);  
        
        }
    else
        {
        draw_sprite(Spr_PlayGameButton,0,272,664);
        };
    //Drawing Title
    draw_sprite(Spr_GameTitle,0,0,0);
};

//GameOver     Menu 2
if menushown = 2
{
    //Drawing Background Black
    draw_set_colour(c_black);
    draw_rectangle(0,0,room_width,room_height,false);
    //Drawing Death Notice
    draw_sprite(Spr_YouDied,0,264,92);    
    //Drawing Stats
    draw_set_colour(c_white);
    draw_set_font(Fnt_Base);
    column1 = (room_width-(string_length("Score")+string_length(string(score))))/5
    rowsep = 1.5*string_height("Score")
    //Score
    draw_text(column1,300,"Score: " + string(score))
    //Zombies Killed
    draw_text(column1,300+rowsep,"Total Kills: " + string(global.TotalKills));
    draw_text(column1,300+(2*rowsep),"Normal: "+string(global.BasicKills));
    draw_text(column1,300+(3*rowsep),"Fast: "+string(global.FastKills));
    draw_text(column1,300+(4*rowsep),"Tough: "+string(global.ToughKills));
    //Shooting Skills
    draw_text(column1,300+(5*rowsep),"Total Damage Dealt: " + string(global.DamageDealt))
    draw_text(column1,300+(6*rowsep),"Total Damage Received: " + string(global.DamageReceived))
    draw_text(column1,300+(7*rowsep),"Total Shots Taken: " + string(global.TotalShots))
    draw_text(column1,300+(8*rowsep),"Total Hits: " + string(global.TotalHits))
    draw_text(column1,300+(9*rowsep),"Hit Percentage: "+string(hitpercentage)+" %")
    //Restarting
    var center = ((room_width-string_length("Press Enter to Retry"))/2)-(5*string_length("Press Enter to Retry"));
    draw_set_colour(c_white);
    draw_text(center,300+(12*rowsep),"Press Enter to Retry")
    if keyboard_check_pressed(vk_enter)
    {room_goto(Room_TestMap)}
    //Ending Game
    if (keyboard_check_pressed(vk_escape))
    {game_end();}
};
</string>
